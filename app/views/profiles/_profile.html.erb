<tr id="<%= dom_id(profile) %>" >

  <% profile.main_attributes.each do |attr_name| %>
  <td><%= show_attr(profile, attr_name) %></td>
  <% end %>

  <td class="index_table-actions">
    <%= link_to_show(profile) %>
    <%= link_to_edit(profile) %>
<% if profile.try(:archived?) %>
    <%= link_to_recover(profile) %>
<% else %>
    <%= link_to_destroy(profile) %>
<% end %>
  </td>
</tr>
