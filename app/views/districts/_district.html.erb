<tr id="<%= dom_id(district) %>" >

  <% district.main_attributes.each do |attr_name| %>
  <td><%= show_attr(district.public_send attr_name) %></td>
  <% end %>

  <td class="index_table-actions">
    <%= link_to_show(district) %>
    <%= link_to_edit(district) %>
<% if district.try(:archived?) %>
    <%= link_to_recover(district) %>
<% else %>
    <%= link_to_destroy(district) %>
<% end %>
  </td>
</tr>
