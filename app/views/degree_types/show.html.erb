<%= main_title(t("action.show", model: DegreeType.model_name.human)) %>

<dl class="dl-horizontal">

  <dt><%= DegreeType.human_attribute_name(:id) %>:</dt>
  <dd><%= @degree_type.id %></dd>
  <dt><%= DegreeType.human_attribute_name(:name) %>:</dt>
  <dd><%= @degree_type.name %></dd>
  <dt><%= DegreeType.human_attribute_name(:educational_type) %>:</dt>
  <dd><%= @degree_type.educational_type %></dd>
  <dt><%= DegreeType.human_attribute_name(:created_at) %>:</dt>
  <dd><%= show_simple_date(@degree_type.created_at) %></dd>

</dl>
<% unless request.xhr? %>
<%= button_to_back %>
<%= button_to_edit(@degree_type) %>
<% end %>