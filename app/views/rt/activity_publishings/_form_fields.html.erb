<div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :organizer %>
  <%= f.input :description %>
  <%= f.input :execution_date, as: :datepicker %>
  <%= f.input :execution_hour %>
  <%= f.input :road_type %>
  <%= f.input :road_name %>
  <%= f.input :number_type %>
  <%= f.input :road_number %>
  <%= f.input :postal_code %>
  <%= f.input :town %>
  <%= f.input :province %>
</div>
<div class="form-actions">
  <%= f.button :submit %>
</div>
