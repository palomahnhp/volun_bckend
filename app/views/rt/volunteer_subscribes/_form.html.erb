<div class="well">
  <%= simple_form_for(@rt_volunteer_subscribe, default_form_options) do |f| %>

  <div class="form-inputs">
          <%= f.input :name %>
          <%= f.input :last_name %>
          <%= f.input :last_name_alt %>
          <%= f.input :phone_number %>
          <%= f.input :email %>
          <%= f.input :publish_pictures %>
          <%= f.input :annual_survey %>
          <%= f.input :notes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
  <% end %>
</div>