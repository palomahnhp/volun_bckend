<tr id="<%= dom_id(volun_availability) %>" >

  <% volun_availability.main_attributes.each do |attr_name| %>
  <td><%= show_attr(volun_availability, attr_name) %></td>
  <% end %>

  <td class="index_table-actions">
    <%= link_to_show(volun_availability) %>
    <%= link_to_edit(volun_availability) %>
<% if volun_availability.try(:archived?) %>
    <%= link_to_recover(volun_availability) %>
<% else %>
    <%= link_to_destroy(volun_availability) %>
<% end %>
  </td>
</tr>
