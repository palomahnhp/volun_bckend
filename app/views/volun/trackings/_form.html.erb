<div class="well">
  <%= simple_form_for(@volun_tracking, default_form_options) do |f| %>

  <div class="form-inputs">
          <%= f.association :volunteer, input_html: { disabled: true }, default: @volun_tracking.volunteer %>
          <%= f.association :tracking_type %>
          <%= f.association :project %>
          <%= f.association :manager, input_html: { disabled: true }, default: @volun_tracking.manager %>
          <%= f.input :tracked_at, as: :string, input_html: date_input_html_default_values(f.object.tracked_at) %>
          <%= f.input :comments %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
  <% end %>
</div>