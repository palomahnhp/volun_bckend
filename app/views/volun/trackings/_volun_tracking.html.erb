<tr id="<%= dom_id(volun_tracking) %>" >

  <% volun_tracking.main_attributes.each do |attr_name| %>
  <td><%= show_attr(volun_tracking.public_send attr_name) %></td>
  <% end %>

  <td class="index_table-actions">
    <%= link_to_show(volun_tracking) %>
    <%= link_to_edit(volun_tracking) %>
<% if volun_tracking.try(:archived?) %>
    <%= link_to_recover(volun_tracking) %>
<% else %>
    <%= link_to_destroy(volun_tracking) %>
<% end %>
  </td>
</tr>
