<tr id="<%= dom_id(rt_entity_unsubscribe) %>" >

  <% rt_entity_unsubscribe.main_attributes.each do |attr_name| %>
  <td><%= show_attr(rt_entity_unsubscribe.public_send attr_name) %></td>
  <% end %>

  <td class="index_table-actions">
    <%= link_to_show(rt_entity_unsubscribe) %>
    <%= link_to_edit(rt_entity_unsubscribe) %>
<% if rt_entity_unsubscribe.try(:archived?) %>
    <%= link_to_recover(rt_entity_unsubscribe) %>
<% else %>
    <%= link_to_destroy(rt_entity_unsubscribe) %>
<% end %>
  </td>
</tr>
