<%= main_title(t("action.show", model: ContactType.model_name.human)) %>

<dl class="dl-horizontal">

  <dt><%= ContactType.human_attribute_name(:id) %>:</dt>
  <dd><%= @contact_type.id %></dd>
  <dt><%= ContactType.human_attribute_name(:name) %>:</dt>
  <dd><%= @contact_type.name %></dd>
  <dt><%= ContactType.human_attribute_name(:active) %>:</dt>
  <dd><%= @contact_type.active %></dd>
  <dt><%= ContactType.human_attribute_name(:created_at) %>:</dt>
  <dd><%= show_simple_date(@contact_type.created_at) %></dd>

</dl>
<% unless request.xhr? %>
<%= button_to_back %>
<%= button_to_edit(@contact_type) %>
<% end %>