<tr id="<%= dom_id(request_form) %>" >

  <% request_form.main_attributes.each do |attr_name| %>
  <td><%= show_attr(request_form, attr_name) %></td>
  <% end %>

  <td class="index_table-actions">
    <%= link_to_show(request_form) %>
    <%= link_to_edit(request_form) %>
<% if request_form.try(:archived?) %>
    <%= link_to_recover(request_form) %>
<% else %>
    <%= link_to_destroy(request_form) %>
<% end %>
  </td>
</tr>
