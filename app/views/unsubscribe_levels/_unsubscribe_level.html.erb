<tr id="<%= dom_id(unsubscribe_level) %>" >

  <% unsubscribe_level.main_attributes.each do |attr_name| %>
  <td><%= show_attr(unsubscribe_level, attr_name) %></td>
  <% end %>

  <td class="index_table-actions">
    <%= link_to_show(unsubscribe_level) %>
    <%= link_to_edit(unsubscribe_level) %>
<% if unsubscribe_level.try(:archived?) %>
    <%= link_to_recover(unsubscribe_level) %>
<% else %>
    <%= link_to_destroy(unsubscribe_level) %>
<% end %>
  </td>
</tr>
