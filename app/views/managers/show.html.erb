<%= main_title(t("action.show", model: Manager.model_name.human)) %>

<dl class="dl-horizontal">

  <dt><%= Manager.human_attribute_name(:id) %>:</dt>
  <dd><%= @manager.id %></dd>
  <dt><%= Manager.human_attribute_name(:name) %>:</dt>
  <dd><%= @manager.name %></dd>
  <dt><%= Manager.human_attribute_name(:profile_id) %>:</dt>
  <dd><%= @manager.profile_id %></dd>
  <dt><%= Manager.human_attribute_name(:phone_number) %>:</dt>
  <dd><%= @manager.phone_number %></dd>
  <dt><%= Manager.human_attribute_name(:active) %>:</dt>
  <dd><%= t("humanize.#{@manager.active}") %></dd>
  <dt><%= Manager.human_attribute_name(:created_at) %>:</dt>
  <dd><%= show_simple_date(@manager.created_at) %></dd>

</dl>
<% unless request.xhr? %>
<%= button_to_back %>
<%= button_to_edit(@manager) %>
<% end %>