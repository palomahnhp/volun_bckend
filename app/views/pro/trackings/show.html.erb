  <dd><%= @pro_tracking.start_date %></dd>
  <dt><%= ProTracking.human_attribute_name(:project_id) %>:</dt>
  <dd><%= @pro_tracking.project %></dd>
  <dt><%= ProTracking.human_attribute_name(:created_at) %>:</dt>
