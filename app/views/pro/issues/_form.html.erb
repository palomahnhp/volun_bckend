<div class="well">
  <%= simple_form_for(@pro_issue, default_form_options) do |f| %>

  <div class="form-inputs">
          <%= f.input :comments %>
          <%= f.input :start_date, as: :datepicker %>
          <%= f.association :project %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
  <% end %>
</div>