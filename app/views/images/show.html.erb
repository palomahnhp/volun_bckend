<%= main_title(t("action.show", model: Image.model_name.human)) %>

<dl class="dl-horizontal">

  <dt><%= Image.human_attribute_name(:id) %>:</dt>
  <dd><%= @image.id %></dd>
  <dt><%= Image.human_attribute_name(:name) %>:</dt>
  <dd><%= @image.name %></dd>
  <dt><%= Image.human_attribute_name(:payload) %>:</dt>
  <dd><%= @image.payload %></dd>
  <dt><%= Image.human_attribute_name(:project_id) %>:</dt>
  <dd><%= @image.project %></dd>
  <dt><%= Image.human_attribute_name(:created_at) %>:</dt>
  <dd><%= show_simple_date(@image.created_at) %></dd>

</dl>
<% unless request.xhr? %>
<%= button_to_back %>
<%= button_to_edit(@image) %>
<% end %>