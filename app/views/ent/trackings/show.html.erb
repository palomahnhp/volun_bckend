<%= main_title(t("action.show", model: EntTracking.model_name.human)) %>

<dl class="dl-horizontal">

  <dt><%= EntTracking.human_attribute_name(:id) %>:</dt>
  <dd><%= @ent_tracking.id %></dd>
  <dt><%= EntTracking.human_attribute_name(:tracking_type_id) %>:</dt>
  <dd><%= @ent_tracking.tracking_type %></dd>
  <dt><%= EntTracking.human_attribute_name(:entity_id) %>:</dt>
  <dd><%= @ent_tracking.entity %></dd>
  <dt><%= EntTracking.human_attribute_name(:manager_id) %>:</dt>
  <dd><%= @ent_tracking.manager %></dd>
  <dt><%= EntTracking.human_attribute_name(:tracked_at) %>:</dt>
  <dd><%= @ent_tracking.tracked_at %></dd>
  <dt><%= EntTracking.human_attribute_name(:comments) %>:</dt>
  <dd><%= @ent_tracking.comments %></dd>
  <dt><%= EntTracking.human_attribute_name(:created_at) %>:</dt>
  <dd><%= show_simple_date(@ent_tracking.created_at) %></dd>

</dl>
<% unless request.xhr? %>
<%= button_to_back %>
<%= button_to_edit(@ent_tracking) %>
<% end %>