<%= simple_form_for(@proposal) do |f| %>
<%= f.error_notification %>

<div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :comments %>
      <%= f.input :registry_date %>
      <%= f.input :approval_date %>
</div>

<div class="form-actions">
  <%= f.button :submit %>
</div>
<% end %>