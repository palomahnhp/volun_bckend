<%= main_title(t("action.show", model: Technician.model_name.human)) %>

<dl class="dl-horizontal">

  <dt><%= Technician.human_attribute_name(:id) %>:</dt>
  <dd><%= @technician.id %></dd>
  <dt><%= Technician.human_attribute_name(:name) %>:</dt>
  <dd><%= @technician.name %></dd>
  <dt><%= Technician.human_attribute_name(:profile_id) %>:</dt>
  <dd><%= @technician.profile_id %></dd>
  <dt><%= Technician.human_attribute_name(:active) %>:</dt>
  <dd><%= t("humanize.#{@technician.active}") %></dd>
  <dt><%= Technician.human_attribute_name(:created_at) %>:</dt>
  <dd><%= show_simple_date(@technician.created_at) %></dd>

</dl>
<% unless request.xhr? %>
<%= button_to_back %>
<%= button_to_edit(@technician) %>
<% end %>